#define min3(a, b, c) min(c, min(a, b))
#define min4(a, b, c, d) min(d, min(c, min(a, b)))
#define rrep(i, n) for(int i=n-1;i>=0;i--)
#define rep(i,n) for(int i=0;i<n;i++)
#define FastHoviBaby ios_base::sync_with_stdio(false), cin.tie(nullptr), cout.tie(nullptr);


int gcd(int a , int b)
{
if(b==0) return a;
a%=b;
return gcd(b,a);
}


bool isPrime(int n){
    if(n==1) return false;
    if(n==2) return true;
    for(int i=2;i*i<=n;i++){
        if(n%i==0)return false;
    }
    return true;
}

int32_t main(){
FastHoviBaby

int n;
cin>>n;
int a[n];
int sum=0;


rep(i,n) {
    cin>>a[i];
    sum+=a[i];
    
}
int mi=a[0];
int flag=0;
rep(i,n){
    rep(j,n){
        if(i==j) continue;
        
        mi= (gcd(mi,a[j]));
        if(a[i]!=a[j]) flag=1;
    }
}
if(!flag) cout<<sum;
else
cout<<mi*n;
return 0;
}

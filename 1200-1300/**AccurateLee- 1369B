    while(r>=l){
        if(s[l]=='1' && s[r]=='0'){
            
            in.pb(r);
            break;
        }else  if(s[l]=='0') {
           in.pb(l);
            l++;
            
        }else if(s[r]=='1') {
            in.pb(r);
            r--;
    }
    }
    sort(all(in));
    rep(i,in.size())
    cout<<s[in[i]];

    cout<<endl;
}


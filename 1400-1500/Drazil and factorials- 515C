int n;
cin>>n;

string s;
cin>>s;

string ans="";
 rep(i,n){
  if(s[i]=='0' || s[i]=='1')
  continue;
  else if( s[i]=='4'){
 ans+="322";
  }else if(s[i]=='6')
  ans+= "35";
 else if(s[i]=='8')
 ans+="2227";
 else if(s[i]=='9')
 ans+= "3327";
else ans+= s[i];
}
  sort(all(ans), greater<char>());
 cout<<ans;


return 0;
}

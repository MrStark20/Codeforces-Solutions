#include <bits/stdc++.h>
#include<string>
#include<stack>
#include<algorithm>
#include<iterator>
#include<cmath>
using namespace std;

#define ll long long
#define ull unsigned long long
#define ppi pair<int,int>

/*:) PHPRULHV EUlQJ EDFN BRX(:
            FOREVER
            DS CPHR

                        */


int main()
{   ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	int t;
	cin>>t;
	while(t--)
    {
        int n,m;
        cin>>n>>m;
        int a[n],b[m];
        for(int i=0;i<n;i++)
            cin>>a[i];
        for(int i=0;i<m;i++)
            cin>>b[i];
        int c=0;
        for(int i=0;i<n;i++)
        {
            for(int j=0;j<m;j++)
            {
                if(a[i]==b[j])
                {c++;
                 break;
                }
            }
        }
        cout<<c<<endl;
    }
return 0;
}

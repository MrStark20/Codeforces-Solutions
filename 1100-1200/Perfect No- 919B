#define fast ios_base::sync_with_stdio(false), cin.tie(nullptr), cout.tie(nullptr);


int gcd(int a , int b)
{
if(b==0) return a;
a%=b;
return gcd(b,a);
}


bool isPrime(int n){
    if(n==1) return false;
    if(n==2) return true;
    for(int i=2;i*i<=n;i++){
        if(n%i==0)return false;
    }
    return true;
}
bool check(int m){
    int ans=0;
   while(m>0){
       ans+=m%10;
       m/=10;
   }
    return ans==10;
}

int32_t main(){
fast

int n;
cin>>n;

int count=0;
for(int i=1;i<=1e9;i++){
   if(check(i))
   count++;
   if(count==n){
       cout<<i<<endl;
       break;
   }
}
    
return 0;
}

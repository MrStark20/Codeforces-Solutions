#define FastHoviBaby ios_base::sync_with_stdio(false), cin.tie(nullptr), cout.tie(nullptr);


int gcd(int a , int b)
{
if(b==0) return a;
a%=b;
return gcd(b,a);
}


bool isPrime(int n){
    if(n==1) return false;
    if(n==2) return true;
    for(int i=2;i*i<=n;i++){
        if(n%i==0)return false;
    }
    return true;
}

int32_t main(){
FastHoviBaby

int n;
cin>>n;

string a[7]= {"", "Sheldon", "Leonard", "Penny", "Rajesh", "Howard" };
 int j=1,l=1;
 if(n<=5){
     cout<<a[n];
 }else{
 for(int i=5;i<n;i+=(j*5)){
     l=i;j*=2;
 }

    int ans= ceil((1.0*(n-l))/j);
    cout<<a[ans];
 }
return 0;
}

#include <bits/stdc++.h>
using namespace std;
#define int long long

int32_t main() {
    int t;cin>>t;
    for(int i=0;i<t;i++){
    int n;cin>>n;int a[n];for(int i=0;i<n;i++)cin>>a[i];
    int f=0;
    for(int i=0;i<n;i++){
        int x=sqrt(a[i]);
        if(x*x != a[i]){
            f=1;
            cout<<"YES"<<endl;
            break;
        }
    }
    if(!f)cout<<"NO"<<endl;
    }
}

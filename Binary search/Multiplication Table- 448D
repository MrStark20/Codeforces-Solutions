#define rrep(i, n) for(int i=n-1;i>=0;i--)
#define rep(i,n) for(int i=0;i<n;i++)
#define fast ios_base::sync_with_stdio(false), cin.tie(nullptr), cout.tie(nullptr);


int gcd(int a , int b)
{
if(b==0) return a;
a%=b;
return gcd(b,a);
}


bool isPrime(int n){
    if(n==1) return false;
    if(n==2) return true;
    for(int i=2;i*i<=n;i++){
        if(n%i==0)return false;
    }
    return true;
}
int n,m,k;


int32_t main(){
fast
cin>>n>>m>>k;
int low=0,high= (n*m)+1000; 

    int count=0;

while(high>=low){
    int mid=(low+high)/2;
    count=0;
     for(int i=1;i<=n;i++){
      
        count+= (min( m, (mid/i)));
          }

    (count>=k) ? (high=mid-1): (low= mid+1) ;
}
cout<<low;

return 0;
}
